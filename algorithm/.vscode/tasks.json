{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"label": "g++.exe build active file",
			"command": "D:\\x86_64 gcc\\mingw64\\bin\\g++.exe",
			"args": [
				"-g",
				"${file}",
				"-o",
